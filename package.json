    "url": "https://devjang.github.io"
  },
  "homepage": "https://nuxt-realworld.vercel.app",
  "description": "Built a Example App of RealWorld with Nuxt & Composition API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devJang/nuxt-realworld.git"
  },
  "bugs": "https://github.com/devJang/nuxt-realworld/issues",
  "keywords": [
    "vue",
    "nuxt",
    "example-project",
    "realworld",
    "typescript",
    "experimental",
    "composition-api"
  ],
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:ts": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js && yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.4",
    "@nuxtjs/composition-api": "^0.17.0",
